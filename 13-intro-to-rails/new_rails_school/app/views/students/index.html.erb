<h1>INDEX PAGE</h1>



<% @students.each do |student| %>
  <p><%# link_to student.name, "/students/#{student.id}" %></p>
  <p><%= link_to student.name, student_path(student.id) %></p>
  <p><%# link_to student.name, student_path(student) %></p>
  <p><%# link_to student.name, student %></p>
<% end %>
